package ch16_2;

public class Ex8 {

	
	private static Student students[]= {
			new Student("홍", 90,96),
			new Student("신", 95,93)
			};
	
	
	
	static double avg(Function<Student> t){
		
		t.apply(null);
		
		return 0.0;
	}
	
	public static void main(String[] args) {
		
		double enAvg=avg(
				null
				);

	}

}
