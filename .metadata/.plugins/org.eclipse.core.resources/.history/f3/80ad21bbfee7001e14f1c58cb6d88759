package ch16;

public class Ex7 {

	static int scores[] = { 10, 50, 3 };

	static int maxOrMin(Operator operator) {

		int result = scores[0];

		for (int score : scores) {
			result=operator.appy(result, score);
		}

		return result;
	}

	public static void main(String[] args) {

		
		//ìµœëŒ€ê°’
		
		maxOrMin(
				(x,y)->{return x+y;}
				);
	}

}
