package jungsuk.ch06;

public class Ex6_22 {
	
	static boolean isNumber(String str){
		return false;
	}
	

	public static void main(String[] args) {
		
		String str = "123";
		System.out.println(str+"는 숫자입니까? "+isNumber(str)); 
		str = "1234o";
		System.out.println(str+"는 숫자입니까? "+isNumber(str)); 
	}

}
